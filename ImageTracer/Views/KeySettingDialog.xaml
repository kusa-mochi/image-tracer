<!-- 画像表示切替ショートカットキー設定画面 -->
<Window x:Class="ImageTracer.Views.KeySettingDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:rs="clr-namespace:ImageTracer.Properties"
        xmlns:local="clr-namespace:ImageTracer.Views"
        mc:Ignorable="d"
        Title="KeySettingDialog" WindowStyle="None" WindowStartupLocation="CenterScreen" WindowState="Maximized" Background="#99000000" AllowsTransparency="True" KeyUp="Window_KeyUp">
    <Grid Height="300" Width="560" Background="White">
        <TextBlock Text="{x:Static rs:Resources.KeySettingDialogLabel_01}" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Window>
